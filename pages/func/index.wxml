<!--pages/func/index.wxml-->
<view class="func">
  <view class="home-func-list">
    <block wx:for="{{funcList}}" wx:key="key">
      <view class="func-item" data-index="{{index}}" bindtap="handlePageTo">
        <view class="icon icon-my">
          <image src="/img/account.png" mode="aspectFit" />
        </view>
        <view class="title">
          {{item.name}}
        </view>
        <view class="right">
          <image src="/img/right.png" mode="aspectFit" />
        </view>
      </view>
    </block>
  </view>
</view>